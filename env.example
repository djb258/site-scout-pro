# Neon Database Connection
NEON_DATABASE_URL=postgresql://user:password@host/database?sslmode=require
NEON_CONNECTION_STRING=postgresql://user:password@host/database?sslmode=require

# Logging
LOG_LEVEL=INFO

# ============================================
# Census Bureau API (FREE)
# ============================================
# Get your key at: https://api.census.gov/data/key_signup.html
CENSUS_API_KEY=your_census_api_key_here

# ============================================
# Google Maps Platform (PAID - ~$200/mo)
# ============================================
# Get at: https://console.cloud.google.com/apis/credentials
# Enable: Places, Directions, Distance Matrix, Street View, Geocoding
GOOGLE_API_KEY=your_google_api_key_here

# ============================================
# Data.gov API Key (FREE - multiple APIs)
# ============================================
# Get at: https://api.data.gov/signup/
# Works with: Recreation.gov, some BLS endpoints
DATA_GOV_API_KEY=your_data_gov_api_key_here

# ============================================
# FBI Crime Data Explorer (FREE - separate key)
# ============================================
# Get at: https://crime-data-explorer.fr.cloud.gov/pages/docApi
FBI_API_KEY=your_fbi_api_key_here

# ============================================
# Recreation.gov (FREE)
# ============================================
# Get at: https://ridb.recreation.gov/docs
RECREATION_GOV_API_KEY=your_recreation_gov_api_key_here

# ============================================
# Firecrawl (PAID - ~$50/mo)
# ============================================
# Get at: https://www.firecrawl.dev/
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# ============================================
# Scraper API (PAID - ~$50/mo)
# ============================================
# Get at: https://www.scraperapi.com/
SCRAPER_API_KEY=your_scraper_api_key_here

# ============================================
# IMO-Creator Integration: n8n Workflow Automation
# ============================================
# n8n is used for workflow automation and orchestration
# Connect to your n8n instance to trigger workflows from IMO-Creator pipelines

# n8n Base URL (your n8n instance)
N8N_BASE_URL=http://localhost:5678

# n8n API Key (get from n8n Settings > API)
N8N_API_KEY=your_n8n_api_key_here

# n8n Webhook URLs (for specific workflows)
# These are generated when you create webhook nodes in n8n
N8N_WEBHOOK_SCREENING=https://your-n8n-instance.com/webhook/screening
N8N_WEBHOOK_SATURATION=https://your-n8n-instance.com/webhook/saturation
N8N_WEBHOOK_SCORING=https://your-n8n-instance.com/webhook/scoring
N8N_WEBHOOK_PARCEL=https://your-n8n-instance.com/webhook/parcel

# n8n Authentication (if using basic auth)
N8N_USERNAME=your_username
N8N_PASSWORD=your_password

# n8n Timeout (seconds)
N8N_TIMEOUT=30

# ============================================
# IMO-Creator Integration: Composio API Orchestration
# ============================================
# Composio is used for API orchestration and integration management
# Connect to Composio to manage external API integrations

# Composio API Key (get from Composio dashboard)
COMPOSIO_API_KEY=your_composio_api_key_here

# Composio Base URL (usually https://api.composio.dev)
COMPOSIO_BASE_URL=https://api.composio.dev

# Composio App IDs (for specific integrations)
# These are created when you connect apps in Composio
COMPOSIO_APP_CENSUS=your_census_app_id
COMPOSIO_APP_UHAUL=your_uhaul_app_id
COMPOSIO_APP_DOT=your_dot_app_id
COMPOSIO_APP_RENT=your_rent_app_id
COMPOSIO_APP_GEOSPATIAL=your_geospatial_app_id

# Composio Workspace ID
COMPOSIO_WORKSPACE_ID=your_workspace_id

# Composio Timeout (seconds)
COMPOSIO_TIMEOUT=30

# Composio Retry Configuration
COMPOSIO_MAX_RETRIES=3
COMPOSIO_RETRY_DELAY=1

