# IMO-Creator Credentials Configuration
# Copy this file to credentials.yaml and fill in your actual credentials
# DO NOT commit credentials.yaml to git

# n8n Configuration
n8n:
  base_url: "http://localhost:5678"
  api_key: "your_n8n_api_key_here"
  username: ""  # Optional, for basic auth
  password: ""  # Optional, for basic auth
  timeout: 30
  webhooks:
    screening: "https://your-n8n-instance.com/webhook/screening"
    saturation: "https://your-n8n-instance.com/webhook/saturation"
    scoring: "https://your-n8n-instance.com/webhook/scoring"
    parcel: "https://your-n8n-instance.com/webhook/parcel"

# Composio Configuration
composio:
  base_url: "https://api.composio.dev"
  api_key: "your_composio_api_key_here"
  workspace_id: "your_workspace_id_here"
  timeout: 30
  max_retries: 3
  retry_delay: 1
  apps:
    census: "your_census_app_id"
    uhaul: "your_uhaul_app_id"
    dot: "your_dot_app_id"
    rent: "your_rent_app_id"
    geospatial: "your_geospatial_app_id"

